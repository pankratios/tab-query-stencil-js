{
  "manifest_version": 2,
  "name": "TabQuery (stenciljs)",
  "description": "",
  "version": "1.0",
  "minimum_chrome_version": "46",
  "content_security_policy": "script-src 'self' 'sha256-OjK60dMW87dfibI70zHl4RSBfXE4ohw9XJoBVVIQttE='",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "images/icon-19.png",
      "38": "images/icon-38.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "history",
    "file:///*",
    "http://*/*",
    "https://*/*"
  ]
}
